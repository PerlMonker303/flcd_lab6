START
DEF N:NUMBER
READ N
DEF NUMBERS:ARRAY[N] OF NUMBER; MAXNUM:NUMBER; IDX:NUMBER
ASSIGN MAXNUM:UNDEFINED
ASSIGN IDX:0
WHILE IDX<N
STARTWHILE
    READ NUMBERS[IDX]
    ASSIGN IDX:(IDX+1)
FINISHWHILE
ASSIGN IDX:0
WHILE IDX <= N
STARTWHILE
    IF MAXNUM === UNDEFINED || NUMBERS[IDX] > MAXNUM
    STARTIF
        ASSIGN MAXNUM:NUMBERS[IDX]
    FINISHIF
    ASSIGN IDX:(IDX+1)
FINISHWHILE
PRINT("The maximum number is " + MAXNUM)
FINISH
